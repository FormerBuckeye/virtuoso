
TD=target-i386
 
CFLAGS= -g

iferret: iferret.o iferret_log.o iferret_op_str.o int_set.o vslht.o
	$(CC) $(CFLAGS) -o iferret iferret.o iferret_log.o iferret_op_str.o int_set.o vslht.o


iferret.o: iferret.c
	$(CC) $(CFLAGS) -c iferret.c 

iferret_log.o: iferret_log.c
	$(CC) $(CFLAGS) -c iferret_log.c 

iferret_op_str.o: $(TD)/iferret_op_str.c
	$(CC) $(CFLAGS) -c $(TD)/iferret_op_str.c 

iferret_syscall.o: $(TD)/iferret_syscall.c
	$(CC) $(CFLAGS) -c $(TD)/iferret_syscall.c 


int_set.o: int_set.c
	$(CC) $(CFLAGS) -c int_set.c 

vslht.o: vslht.c
	$(CC) $(CFLAGS) -c vslht.c 


clean:
	rm -f iferret.o iferret_log.o  $(TD)/iferret_op_str.o int_set.o

